{
    "$comment": "Source, modified to support verbose output: https://gitlab.com/Linaro/WindowsPerf/windowsperf/-/blob/dc29da908d7e0bb48a0d6422049000bac831dff4/wperf-scripts/tests/schemas/wperf.list.schema",
    "type": "object",
    "required": ["Predefined_Events", "Predefined_Metrics"],
    "additionalProperties": false,
    "properties": {
        "Predefined_Events": {
            "type": "array",
            "uniqueItems": true,
            "items": {
                "type": "object",
                "required": ["Alias_Name", "Event_Type", "Raw_Index"],
                "additionalProperties": false,
                "properties": {
                    "Alias_Name": {
                        "type": "string"
                    },
                    "Event_Type": {
                        "type": "string"
                    },
                    "Raw_Index": {
                        "type": "string"
                    },
                    "Description": {
                        "type": "string"
                    }
                }
            }
        },
        "Predefined_Metrics": {
            "type": "array",
            "uniqueItems": true,
            "items": {
                "type": "object",
                "required": ["Events", "Metric"],
                "additionalProperties": false,
                "properties": {
                    "Events": {
                        "type": "string"
                    },
                    "Metric": {
                        "type": "string"
                    },
                    "Formula": {
                        "type": "string"
                    },
                    "Unit": {
                        "type": "string"
                    },
                    "Description": {
                        "type": "string"
                    }
                }
            }
        },
        "Predefined_Groups_of_Metrics": {
            "type": "array",
            "uniqueItems": true,
            "items": {
                "type": "object",
                "required": ["Group", "Metrics"],
                "additionalProperties": false,
                "properties": {
                    "Group": {
                        "type": "string"
                    },
                    "Metrics": {
                        "type": "string"
                    },
                    "Description": {
                        "type": "string"
                    }
                }
            }
        }
    }
}
